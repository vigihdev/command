{
    "name": "vigihdev/command",
    "description": "PHP Command Library - A powerful Symfony Console-based command library with enhanced features for process management, filesystem operations, and configuration handling",
    "type": "library",
    "keywords": [
        "command",
        "console",
        "cli",
        "symfony",
        "process",
        "filesystem",
        "configuration",
        "yaml",
        "encryption"
    ],
    "homepage": "https://github.com/vigihdev/command",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Vigih Dev",
            "email": "vigihdev@gmail.com",
            "role": "developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "Vigihdev\\Command\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vigihdev\\Command\\Tests\\": "tests/"
        }
    },
    "config": {
        "process-timeout": 0
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://satis.id"
        }
    ],
    "scripts": {
        "test": "phpunit",
        "dev": "php -S localhost:3232 dev.php"
    },
    "require": {
        "php": "^8.1",
        "symfony/console": "^6.4",
        "symfony/process": "^6.4",
        "symfony/filesystem": "^6.4",
        "symfony/finder": "^6.4",
        "symfony/yaml": "^6.4",
        "vigihdev/symfony-bridge-config": "^1.0",
        "vigihdev/serializer": "dev-main",
        "guzzlehttp/guzzle": "^7.10",
        "vigihdev/encryption": "dev-main",
        "vigihdev/support": "dev-main"
    },
    "require-dev": {
        "phpunit/phpunit": "10.5.x-dev"
    }
}
